<Page
    x:Class="PiaoliuHKOperator.Views.runScript"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:PiaoliuHKOperator.Views"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d">

    <Grid Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">
        <Button x:Name="run_Button" Content="RUN" HorizontalAlignment="Left" Margin="299,10,0,0" VerticalAlignment="Top" Click="run_Button_Click"/>
        <CheckBox x:Name="IDRepeatChecks_CheckBox" Content="IDRepeatChecks" HorizontalAlignment="Left" Margin="10,5,0,0" VerticalAlignment="Top" IsChecked="True"/>
        <CheckBox x:Name="checkBox_Copy" Content="CheckBox" HorizontalAlignment="Left" Margin="10,37,0,0" VerticalAlignment="Top"/>
        <CheckBox x:Name="INSYSPackageRelatedTransitBillCheck_CheckBox" Content="INSYSPackageRelatedTransitBillCheck" HorizontalAlignment="Left" Margin="10,69,0,0" VerticalAlignment="Top"/>
        <CheckBox x:Name="SIGNEDPackageRelatedTransitBillCheck_CheckBox" Content="SIGNEDPackageRelatedTransitBillCheck" HorizontalAlignment="Left" Margin="10,101,0,0" VerticalAlignment="Top"/>
        <TextBlock x:Name="Return_TextBlock" Margin="10,350,10,10" TextWrapping="Wrap" Text="">
        </TextBlock>

    </Grid>
</Page>
